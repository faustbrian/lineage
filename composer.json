{
    "name": "cline/lineage",
    "description": "Closure table hierarchies for Eloquent models with O(1) ancestor/descendant queries.",
    "license": "MIT",
    "keywords": [
        "ancestors",
        "closure-table",
        "descendants",
        "eloquent",
        "hierarchy",
        "laravel",
        "nested",
        "tree"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.4.0",
        "cline/morpheus": "^1.0.1",
        "illuminate/container": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "cline/php-cs-fixer": "^1.2.1",
        "driftingly/rector-laravel": "^2.1.3",
        "ergebnis/composer-normalize": "^2.49@dev",
        "larastan/larastan": "^3.8",
        "laravel/pint": "^1.25.1",
        "orchestra/testbench": "^10.7",
        "pestphp/pest": "^4.1.4",
        "pestphp/pest-plugin-drift": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0.3",
        "phpstan/phpstan": "^2.1.32",
        "rector/rector": "^2.2.8",
        "symfony/var-dumper": "^7.3.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\Lineage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Lineage": "Cline\\Lineage\\Facades\\Lineage"
            },
            "providers": [
                "Cline\\Lineage\\LineageServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix",
        "refactor": "rector",
        "test": "@test:morphs",
        "test:lint": "vendor/bin/php-cs-fixer check --diff",
        "test:morphs": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:numeric": [
            "@test:lint",
            "@test:type-coverage:numeric",
            "@test:unit:numeric",
            "@test:types",
            "@test:refactor"
        ],
        "test:refactor": "rector --dry-run",
        "test:seq:morphs": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit:seq",
            "@test:types",
            "@test:refactor"
        ],
        "test:seq:numeric": [
            "@test:lint",
            "@test:type-coverage:numeric",
            "@test:unit:seq:numeric",
            "@test:types",
            "@test:refactor"
        ],
        "test:seq:ulid": [
            "@test:lint",
            "@test:type-coverage:ulid",
            "@test:unit:seq:ulid",
            "@test:types",
            "@test:refactor"
        ],
        "test:seq:uuid": [
            "@test:lint",
            "@test:type-coverage:uuid",
            "@test:unit:seq:uuid",
            "@test:types",
            "@test:refactor"
        ],
        "test:type-coverage": "vendor/bin/pest --parallel --min=94.6 --type-coverage --configuration=phpunit.morphs.xml",
        "test:type-coverage:numeric": "vendor/bin/pest --parallel --min=94.6 --type-coverage --configuration=phpunit.numeric.xml",
        "test:type-coverage:ulid": "vendor/bin/pest --parallel --min=94.6 --type-coverage --configuration=phpunit.ulid.xml",
        "test:type-coverage:uuid": "vendor/bin/pest --parallel --min=94.6 --type-coverage --configuration=phpunit.uuid.xml",
        "test:types": "phpstan --memory-limit=4G",
        "test:ulid": [
            "@test:lint",
            "@test:type-coverage:ulid",
            "@test:unit:ulid",
            "@test:types",
            "@test:refactor"
        ],
        "test:unit": "pest --parallel --min=94.6 --coverage --configuration=phpunit.morphs.xml",
        "test:unit:numeric": "pest --parallel --min=94.6 --coverage --configuration=phpunit.numeric.xml",
        "test:unit:seq": "pest --parallel --processes=1 --min=94.6 --coverage --configuration=phpunit.morphs.xml",
        "test:unit:seq:numeric": "pest --parallel --processes=1 --min=94.6 --coverage --configuration=phpunit.numeric.xml",
        "test:unit:seq:ulid": "pest --parallel --processes=1 --min=94.6 --coverage --configuration=phpunit.ulid.xml",
        "test:unit:seq:uuid": "pest --parallel --processes=1 --min=94.6 --coverage --configuration=phpunit.uuid.xml",
        "test:unit:ulid": "pest --min=94.6 --coverage --configuration=phpunit.ulid.xml",
        "test:unit:uuid": "pest --parallel --min=94.6 --coverage --configuration=phpunit.uuid.xml",
        "test:uuid": [
            "@test:lint",
            "@test:type-coverage:uuid",
            "@test:unit:uuid",
            "@test:types",
            "@test:refactor"
        ]
    }
}
